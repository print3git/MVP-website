{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmit": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true
  },
  "include": ["backend/utils/generateTitle.js"],
  "exclude": ["node_modules", "img", "models", "uploads"]
}
